fun fib(n) {
  print n;
  if (n <= 1) return n;
  var n1 = fib(n - 2);
  print "after n - 2, n: " + n;
  var n2  = fib(n - 2);

  return n1 + n2;
}

for (var i = 0; i < 20; i = i + 1) {
  print "fib " + i + " " + fib(i);
}

